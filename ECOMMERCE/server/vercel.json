{
    "installCommand":"npm install",
    "version":2,
    "builds": [
      { 
        "src": "index.js", 
        "use": "@vercel/node"
      },
      { 
        "src": "api/**/*", 
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/" }

    ]
  }